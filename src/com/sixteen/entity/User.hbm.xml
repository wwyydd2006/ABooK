<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sixteen.entity">
	<class name="User" table="t_user" >
		<id name="uid">
			<generator class="native" />
		</id>
        <property name="uname" column="uname" />
		<property name="upasswd" column="upasswd" />
		<property name="unickname" column="unickname" />
		<property name="uhead" column="uhead" />
		<property name="ugender" column="ugender" />
		<property name="usignature" column="usignature" />
		<property name="upower" column="upower" />
	</class>
</hibernate-mapping>